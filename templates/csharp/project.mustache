<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <Authors>{{info.x-authors}}</Authors>
    <AssemblyTitle>{{info.x-assemblyTitle}}</AssemblyTitle>
    <Company>{{info.x-company}}</Company>
    <Description>{{info.description}}</Description>
    <Title>{{info.title}}</Title>
    <TermsOfService>{{info.termsOfService}}</TermsOfService>
    <ContactEmail>{{info.contact.email}}</ContactEmail>
    <ContactUrl>{{info.contact.url}}</ContactUrl>
    <LicenseName>{{info.license.name}}</LicenseName>
    <LicenseUrl>{{info.license.url}}</LicenseUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Http" Version="6.0.0" />
    <PackageReference Include="System.Text.Json" Version="6.0.0" />
  </ItemGroup>

</Project>
